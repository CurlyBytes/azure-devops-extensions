{
  "manifestVersion": 1,
  "id": "azure-subscription-deploy",
  "version": "0.1.0",
  "name": "Azure Subscription Deploy",
  "description": "Deploy ARM template to Azure Subscription",
  "publisher": "JamieTaffurelli",
  "public": false,
  "categories": ["Azure Pipelines"],
  "scopes": ["vso.release", "vso.serviceendpoint"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/logo.png"
  },
  "tags": ["release", "build", "Azure", "Resource", "Subscription", "ARM", "Deploy"],
  "screenshots": [],
  "branding": {
    "color": "rgb(0, 0, 0)",
    "theme": "dark"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/JamieTaffurelli/azure-devops-extensions/blob/master/subscription-deploy/README.md"
    },
    "issues": {
      "uri": "https://github.com/JamieTaffurelli/azure-devops-extensions/issues"
    },
    "license": {
      "uri": "https://github.com/JamieTaffurelli/azure-devops-extensions/blob/master/subscription-deploy/LICENSE"
    },
    "repository": {
      "uri": "https://github.com/JamieTaffurelli/azure-devops-extensions"
    },
    "support": {
      "uri": "https://github.com/JamieTaffurelli/azure-devops-extensions/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/JamieTaffurelli/azure-devops-extensions"
  },
  "files": [
    {
      "path": "tasks/subscription-deploy/v1"
    },
    {
      "path": "images/logo.png",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "azure-subscription-deploy",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/subscription-deploy/v1"
      }
    }
  ]
}
